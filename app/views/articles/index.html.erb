<div class="container">
  <div class="row articles">
    <h1>Articles</h1>
    <div class="col-sm-10 article">
      <% @articles.each do |article| %>
        <strong><%= link_to article.title, article %></strong>
        <br>
        <p><%= article.body %></p>
      <% end %>
    </div>
  </div>
</div>



<br>

